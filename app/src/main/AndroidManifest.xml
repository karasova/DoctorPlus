<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.doctorplus">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StartActivity"></activity>
        <activity android:name=".EnterActivity"></activity>
        <activity android:name=".OpenCallsActivity"></activity>
        <activity android:name=".ProfileActivity"></activity>
        <activity android:name=".InfoActivity"></activity>
        <activity android:name=".SettingsActivity"></activity>
        <activity android:name=".ActiveCallActivity"></activity>

    </application>

</manifest>